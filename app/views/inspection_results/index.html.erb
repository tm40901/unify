<h1>点検結果一覧</h1>

<table class="table table-hover">
  <thead class="table-info">
    <tr>
      <th>No.</th>
      <th>計測器</th>
      <th>点検者</th>
      <th>点検日時</th>
    </tr>
  </thead>
  <tbody>
    <% @inspection_results.each do |custom_id, results| %>
      <tr>
        <td><%= custom_id %></td>
        <td><%= results.first.instrument.name %></td>
        <td><%= results.first.inspector.last_name + " " + results.first.inspector.first_name %></td>
        <td><%= l results.first.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>